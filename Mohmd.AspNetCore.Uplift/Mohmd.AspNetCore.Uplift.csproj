<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <Version>1.0.3</Version>
    <Authors>Mohammad Azhdari</Authors>
    <Product>Uplift</Product>
    <Description>Easily catch uploaded files in ASP.NET Core 2.0 and later</Description>
    <PackageLicenseExpression />
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <PackageProjectUrl>https://github.com/azhdari/uplift</PackageProjectUrl>
    <RepositoryUrl>https://github.com/azhdari/uplift</RepositoryUrl>
    <RepositoryType>GIT</RepositoryType>
    <Copyright>Copyright (c) 2019 Mohammad Azhdari</Copyright>
    <PackageTags>aspnetcore, upload, files</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Mvc.Core" Version="2.0.1" />
  </ItemGroup>


  <ItemGroup>
    <Folder Include="Internal\" />
    <Folder Include="TagHelpers\" />
  </ItemGroup>


  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
